{
  "name": "gist",
  "version": "1.0.0",
  "description": "Gist",
  "main": "src/index.js",
  "scripts": {
    "build": "mkdir -p dist ; browserify -t babelify src/index.js | derequire > dist/index.js ; cat src/index.scss | node-sass > dist/index.css",
    "watch": "watchify -t babelify src/index.js -o 'derequire > dist/index.js' -v"
  },
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "babel-preset-es2015": "^6.9.0",
    "babelify": "^7.3.0",
    "browserify": "^13.0.1",
    "derequire": "^2.0.3",
    "diffhtml-inline-transitions": "^1.1.0",
    "watchify": "^3.7.0"
  },
  "dependencies": {
    "diffhtml": "^0.9.2",
    "redux": "^3.5.2",
    "transform-tagged-diffhtml": "^1.0.0-rc.3"
  }
}
